<h1>Productos</h1>
<table id="main">
    <tr>
       <th>Titulo:</th>
       <th>Descripción:</th> 
       <th>Precio:</th> 
       <th>Código:</th>  
       <th>Stock:</th> 
    </tr>
    {{#each products}}
    <tr> 
        <td>{{this.title}}</td>
        <td>{{this.description}}</td>
        <td>{{this.price}}</td>
        <td>{{this.code}}</td>
        <td>{{this.stock}}</td>
    </tr>
    {{/each}}
</table>